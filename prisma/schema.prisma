// This repo uses the shared Prisma schema at:
// ../packages/shared-prisma-schema/schema.prisma
//
// Do NOT define models here.

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@adam/shared-prisma-client"
}
